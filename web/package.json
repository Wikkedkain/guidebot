{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "postcss css/tailwind.css -o ../public/build/bundle.css -w",
    "build:tailwind": "SET NODE_ENV=production&& postcss css/tailwind.css -o ../public/build/bundle.css",
    "build": "npm run build:tailwind && rollup -c",
    "dev": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^8.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^1.9.6"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.4.2"
  }
}
